    "gulp-clean": "0.4.0",
    "jest": "24.9.0",
    "prettier": "1.19.1",
    "ts-jest": "24.1.0",
    "ts-loader": "6.2.1",
    "ts-node": "8.5.2",
    "tslint": "5.20.1",
    "typescript": "3.7.2"
  },
  "peerDependencies": {
    "typescript": "^3.6.4"
  }
